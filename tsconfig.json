{
	"compilerOptions": {
		"target": "es6",
		"module": "amd",
		"lib": [
			"es2016",
			"dom"
		],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"strictNullChecks": true,
		"stripInternal": true,
		"removeComments": true,
		"declaration": true,
		//"outDir": "tmp"
		"outFile": "tmp/tswlib.js"
	},
	"exclude": [
		"out",
		"tmp"
	]
}